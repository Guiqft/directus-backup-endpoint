{
    "name": "directus-backup-endpoint",
    "description": "Custom endpoint to backup the database and upload the file to Directus.",
    "version": "1.0.0",
    "keywords": [
        "directus",
        "directus-extension",
        "directus-custom-endpoint"
    ],
    "directus:extension": {
        "type": "endpoint",
        "path": "index.js",
        "source": "src/index.ts",
        "host": "^9.17.4"
    },
    "scripts": {
        "build": "directus-extension build",
        "dev": "directus-extension build -w --no-minify"
    },
    "devDependencies": {
        "@directus/extensions-sdk": "9.17.4",
        "@types/express": "4.17.14",
        "@types/node": "18.7.18",
        "directus": "9.17.4",
        "dotenv": "16.0.2",
        "express": "4.18.1",
        "typescript": "4.8.3"
    },
    "dependencies": {
        "@getvim/execute": "1.0.0"
    }
}
