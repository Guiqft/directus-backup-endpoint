{
    "name": "directus-backup-operation",
    "description": "Custom operation to backup postgres database and upload the file to Directus.",
    "version": "1.0.0",
    "keywords": [
        "directus",
        "directus-extension",
        "directus-custom-operation"
    ],
    "directus:extension": {
        "type": "operation",
        "path": {
            "app": "app.js",
            "api": "api.js"
        },
        "source": {
            "app": "src/app.ts",
            "api": "src/api.ts"
        },
        "host": "^9.18.0"
    },
    "scripts": {
        "build": "directus-extension build",
        "dev": "directus-extension build -w --no-minify"
    },
    "devDependencies": {
        "@directus/extensions-sdk": "9.18.0",
        "@types/node": "18.7.18",
        "directus": "9.17.4",
        "typescript": "4.8.3"
    }
}
